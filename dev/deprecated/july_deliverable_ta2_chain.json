{
  "chain_name": "july_delivery_ta2",
  "algorithms": [
    {
      "name": "kitware/stac_ingress",
      "parameters": {
        "stac_api_url": "https://api.smart-stac.com/",
        "stac_api_key": "$STAC_API_KEY",
        "date_range": [
          "2019-01-01",
          "2020-01-01"
        ],
        "collections": "worldview-nitf",
        "aoi_bounds": [
          124,
          37,
          131,
          43
        ],
        "dry_run": 0,
        "max_results": 2,
        "output_dir": "$CI_PROJECT_DIR/stac_ingress"
      }
    },
    {
      "name": "kitware/stac_to_kwcoco",
      "parameters": {
        "kwcoco": "$CI_PROJECT_DIR/kwcoco_out.json"
      }
    },
    {
      "name": "kitware/random_detections",
      "parameters": {
        "output_dir": "$CI_PROJECT_DIR/random_detections"
      }
    },
    {
      "name": "kitware/kwcoco_to_geojson",
      "parameters": {
        "output_dir": "$CI_PROJECT_DIR/kwcoco_to_geojson",
        "region_id": "1"
      }
    }
  ]
}
